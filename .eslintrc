{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true,
    },
    "plugins": [
      "react",
    ],
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "padded-blocks": 0,
      "arrow-body-style": 0,
    }
}
